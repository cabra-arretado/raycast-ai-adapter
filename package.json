{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ai-adapter",
  "title": "AI Adapter",
  "description": "",
  "icon": "command-icon.png",
  "author": "cabra-arretado",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "AI Adapter",
      "description": "Template for showing typeahead search with network results.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "type": "password",
      "name": "api_key",
      "required": true,
      "title": "API Key",
      "description": "API Key for AI Adapter",
      "defaultValue": ""
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.3",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
